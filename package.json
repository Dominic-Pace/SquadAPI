{
  "name": "squad-api",
  "version": "0.0.1",
  "description": "API for Squad",
  "main": "dist",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w app --exec \"babel-node app --presets es2015,stage-0\"",
    "build": "npm install pm2 -g babel app -s D -d dist --presets es2015,stage-0",
    "start": "NODE_ENV=development pm2 start dist",
    "preinstall": "npm install babel-cli babel-core babel-preset-es2015 babel-preset-stage-0 --save",
    "prestart": "npm run -s build",
    "lint": "eslint app",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dominic-Pace/SquadAPI.git"
  },
  "keywords": [
    "mongo"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Dominic Pace <dpace@nhpace.net>",
  "bugs": {
    "url": "https://github.com/Dominic-Pace/SquadAPI/issues"
  },
  "homepage": "https://github.com/Dominic-Pace/SquadAPI#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^7.4.1",
    "moment": "^2.18.1",
    "mongoose": "^4.10.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "swagger-jsdoc": "^1.9.4",
    "swagger-ui-express": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.19.0",
    "nodemon": "^1.11.0"
  }
}
